#include<stdio.h>
#include<conio.h>
void main()
{	
	int a[50],n,i,j,temp=0;
	printf("enter how many numbers to sort:\n");
	scanf("%d",&n);
	printf("enter the %d numbers to sort:\n",n);
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	for(i=0;i<n;i++)
	{
		for(j=i+1;j<n;j++)
		{if(a[i]>a[j])
		{temp=a[i];
		a[i]=a[j];
		a[j]=temp;
		}
	  }
	}
	printf("\n\n\n\t\tThe sorted number using Buble sort is:\n");
	for(i=0;i<n;i++)
	printf("\n\t\t%d",a[i]);
	
}
